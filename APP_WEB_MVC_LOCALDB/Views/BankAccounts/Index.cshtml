@model IEnumerable<APP_WEB_MVC_LOCALDB.Models.DatosBancarios>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.banco)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sucursal)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.num_cuenta)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.banco)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sucursal)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.num_cuenta)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
